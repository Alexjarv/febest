<!DOCTYPE html>
<html lang="en-us">
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Febest Blog{% endblock %}</title>
        <meta name="title" content="Febest Blog">
        <meta http-equiv="content-language" content="en-us">
        <meta name="description" content="Febest Blog Test Service">
        <meta name="viewport" content="user-scalable=no, width=device-width, initial-scale=1, maximum-scale=1">
        <meta name="author" content="AlexJarv">
        <meta name="generator" content="AlexJarv">
        <link rel="icon" type="image/png" href="{{ asset('assets/images/logo.jpg') }}">
        <link rel="shortcut icon" href="{{ asset('assets/images/logo.jpg') }}" type="image/png">
    </head>
    <body>
        {% block body %}{% endblock %}
    </body>
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}
</html>
